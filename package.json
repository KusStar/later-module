{
  "name": "lazy-module",
  "version": "1.0.0",
  "description": "Download and load npm module lazily",
  "keywords": [
    "lazy-module",
    "lazy download",
    "lazy load",
    "lazy require"
  ],
  "repository": "kusstar/lazy-module",
  "license": "MIT",
  "author": "KusStar",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "pub": "npm run build && releast-it"
  },
  "dependencies": {
    "pacote": "^13.6.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/pacote": "^11.1.5",
    "@vercel/ncc": "^0.34.0",
    "release-it": "^15.1.1",
    "tslib": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  }
}
